<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ManeuverApproachBar">
        <attr name="orientation" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FontTextView">
        <attr name="fontName" format="string" />
    </declare-styleable>

    <declare-styleable name="MQTextView">
        <attr name="MQTypeface">
            <enum name="system"   value="0" />
            <enum name="symbol"   value="1" />
            <enum name="mqLogo"   value="2" />
            <enum name="semibold" value="3" />
            <enum name="regular"  value="4" />
            <enum name="weather"  value="5" />
            <enum name="bold"     value="6" />
        </attr>
        <attr name="textAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="SearchBarView">
        <attr name="placeHolder" format="boolean"/>
    </declare-styleable>
</resources>
